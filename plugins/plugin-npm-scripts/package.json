{
  "name": "@dash4/plugin-npm-scripts",
  "version": "0.1.0",
  "private": false,
  "description": "dash4 plugin npm scripts",
  "repository": "https://github.com/smollweide/dash4",
  "license": "MIT",
  "author": "Simon Mollweide<simon.mollweide@gmail.com>",
  "files": [
    "build",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "main": "./build/server/index.js",
  "typings": "./build/server/index.d.ts",
  "scripts": {
    "analyze-bundle-size": "node ./node_modules/.bin/dash4-analyze-bundle-size",
    "prebuild": "node ./node_modules/.bin/dash4-prebuild",
    "build": "dash4-npm-run-all build:*",
    "build:client": "node ./node_modules/.bin/dash4-build-client",
    "build:server": "node ./node_modules/.bin/dash4-build-server",
    "prepublishOnly": "dash4-npm-run-all build",
    "start": "node ./node_modules/.bin/dash4-start",
    "static": "node ./node_modules/.bin/dash4-static",
    "test": "dash4-npm-run-all test:*",
    "test-update-snapshots:client": "node ./node_modules/.bin/dash4-test-client-update-snapshots",
    "test:client": "node ./node_modules/.bin/dash4-test-client",
    "test:server": "node ./node_modules/.bin/dash4-test-server",
    "watch-build": "node ./node_modules/.bin/dash4-watch-build",
    "watch-dist": "node ./node_modules/.bin/dash4-watch-dist",
    "watch-test-client": "node ./node_modules/.bin/dash4-watch-test-client",
    "watch-test-server": "node ./node_modules/.bin/dash4-watch-test-server"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@dash4/client": "0.1.0",
    "@dash4/server": "0.1.0",
    "@dash4/terminal-emulator": "0.1.0",
    "@dash4/ui": "0.1.0"
  },
  "devDependencies": {
    "@dash4/config": "0.1.1",
    "@dash4/react-xterm": "0.1.0",
    "@types/core-js": "2.5.0",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.2",
    "@types/react": "16.8.13",
    "@types/react-dom": "16.8.3",
    "chalk": "2.4.2",
    "columnify": "1.5.4",
    "core-js": "3.0.1",
    "jest": "24.7.1",
    "ora": "3.4.0",
    "react": "16.8.6",
    "react-bootstrap": "1.0.0-beta.6",
    "react-dom": "16.8.6",
    "react-hooks-testing-library": "0.4.0",
    "react-jss": "10.0.0-alpha.10",
    "react-testing-library": "6.1.2",
    "sparkly": "4.0.0",
    "ts-jest": "24.0.2"
  }
}