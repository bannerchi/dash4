{
  "name": "@dash4/plugin-terminal",
  "description": "dash4 plugin - terminal widget",
  "version": "0.1.0",
  "author": "Simon Mollweide<simon.mollweide@gmail.com>",
  "license": "MIT",
  "private": false,
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "scripts": {
    "prepublishOnly": "run-s build",
    "prebuild": "rimraf build dist",
    "build": "run-p build:*",
    "build:client": "webpack --mode production",
    "build:server": "tsc --outDir build --project tsconfig.server.json",
    "start": "cross-env NODE_ENV=development nodemon --watch ./node_modules/@dash4/server/build ./node_modules/@dash4/server/build",
    "start2": "webpack-dev-server --mode development",
    "watch-build": "tsc --watch --outDir build --project tsconfig.server.json",
    "watch-dist": "webpack --mode production --watch"
  },
  "devDependencies": {
    "@namics/ts-config": "0.2.0",
    "@types/core-js": "2.5.0",
    "@types/react": "16.8.3",
    "@types/react-dom": "16.8.2",
    "@types/react-bootstrap": "0.32.17",
    "asset-config-webpack-plugin": "1.3.1",
    "clean-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "3.2.0",
    "ts-config-webpack-plugin": "1.3.1",
    "scss-config-webpack-plugin": "1.3.1",
    "npm-run-all": "4.1.5",
    "rimraf": "2.6.3",
    "typescript": "3.3.3",
    "webpack": "4.29.4",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.1.14",
    "core-js": "2.6.3",
    "react": "16.8.2",
    "react-dom": "16.8.2",
    "react-bootstrap": "1.0.0-beta.5",
    "nodemon": "1.18.10",
    "cross-env": "5.2.0",
    "chalk": "2.4.2",
    "ora": "3.1.0",
    "sparkly": "4.0.0",
    "columnify": "1.5.4"
  },
  "dependencies": {
    "@dash4/client": "0.1.0",
    "@dash4/server": "0.1.0",
    "xterm": "3.11.0",
    "node-pty": "0.8.1",
    "os": "0.1.1"
  }
}