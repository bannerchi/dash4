{
  "name": "@dash4/plugin-terminal",
  "version": "0.1.0",
  "private": false,
  "description": "dash4 plugin terminal",
  "license": "MIT",
  "author": "Simon Mollweide<simon.mollweide@gmail.com>",
  "main": "./build/server/index.js",
  "typings": "./build/server/index.d.ts",
  "scripts": {
    "analyze-bundle-size": "node ./node_modules/.bin/dash4-analyze-bundle-size",
    "prebuild": "node ./node_modules/.bin/dash4-prebuild",
    "build": "dash4-run-s build:*",
    "build:client": "node ./node_modules/.bin/dash4-build-client",
    "build:server": "node ./node_modules/.bin/dash4-build-server",
    "prepublishOnly": "dash4-run-s build",
    "start": "node ./node_modules/.bin/dash4-start",
    "static": "node ./node_modules/.bin/dash4-static",
    "watch-build": "node ./node_modules/.bin/dash4-watch-build",
    "watch-dist": "node ./node_modules/.bin/dash4-watch-dist"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@dash4/client": "0.1.0",
    "@dash4/react-xterm": "0.1.0",
    "@dash4/server": "0.1.0",
    "node-pty": "0.8.1",
    "os": "0.1.1"
  },
  "devDependencies": {
    "@dash4/config": "0.1.0",
    "@types/core-js": "2.5.0",
    "@types/node": "11.11.3",
    "@types/react": "16.8.8",
    "@types/react-dom": "16.8.2",
    "chalk": "2.4.2",
    "columnify": "1.5.4",
    "core-js": "2.6.5",
    "ora": "3.1.0",
    "react": "16.8.4",
    "react-bootstrap": "1.0.0-beta.6",
    "react-dom": "16.8.4",
    "react-jss": "10.0.0-alpha.10",
    "sparkly": "4.0.0"
  }
}