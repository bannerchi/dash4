{
  "name": "@dash4/ui",
  "version": "0.2.1",
  "private": false,
  "description": "dash4 react ui component",
  "repository": "https://github.com/smollweide/dash4",
  "license": "MIT",
  "author": "Simon Mollweide<simon.mollweide@gmail.com>",
  "files": [
    "build",
    "lib",
    "LICENSE",
    "README.md"
  ],
  "main": "./build/index.js",
  "typings": "./build/index.d.ts",
  "scripts": {
    "analyze-bundle-size": "cross-env NODE_ENV=production ANALYZE_ENV=bundle webpack --config webpack.library.config.js --mode production",
    "build": "npm-run-all build:*",
    "build-storybook": "build-storybook",
    "build:build": "tsc --outDir build",
    "build:lib": "webpack --config webpack.library.config.js --mode production",
    "postinstall": "npm rebuild node-sass",
    "start": "start-storybook -p 6006",
    "test": "jest --coverage --config=jest.config.js",
    "test-update-snapshots": "jest --coverage --update-snapshot --config=jest.config.js",
    "watch-build": "run-p watch-build:*",
    "watch-build:build": "tsc --watch --outDir build",
    "watch-build:lib": "webpack --watch --config webpack.library.config.js --mode production",
    "watch-test": "jest --coverage --watchAll --config=jest.config.js"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@rooks/use-key": "3.0.1",
    "react-jss": "10.0.0-alpha.10"
  },
  "devDependencies": {
    "@dash4/config": "0.2.1",
    "@storybook/addon-actions": "5.0.6",
    "@storybook/addon-info": "5.0.6",
    "@storybook/addon-knobs": "5.0.6",
    "@storybook/addon-links": "5.0.6",
    "@storybook/addons": "5.0.6",
    "@storybook/react": "5.0.6",
    "@types/jest": "24.0.11",
    "@types/react": "16.8.13",
    "@types/react-dom": "16.8.4",
    "@types/storybook-addon-jsx": "5.4.3",
    "@types/storybook-readme": "5.0.2",
    "@types/storybook__addon-actions": "3.4.2",
    "@types/storybook__addon-info": "4.1.1",
    "@types/storybook__addon-knobs": "4.0.5",
    "@types/storybook__addon-links": "3.3.4",
    "@types/storybook__react": "4.0.1",
    "asset-config-webpack-plugin": "1.3.1",
    "bootstrap": "4.3.1",
    "clean-webpack-plugin": "2.0.1",
    "core-js": "3.0.1",
    "cross-env": "5.2.0",
    "html-webpack-plugin": "3.2.0",
    "jest": "24.7.1",
    "npm-run-all": "4.1.5",
    "react": "16.8.6",
    "react-bootstrap": "1.0.0-beta.8",
    "react-hooks-testing-library": "0.4.0",
    "react-testing-library": "6.1.2",
    "scss-config-webpack-plugin": "1.3.1",
    "storybook-addon-jsx": "7.1.0",
    "storybook-readme": "5.0.2",
    "ts-config-webpack-plugin": "1.3.1",
    "ts-jest": "24.0.2",
    "typescript": "3.4.3",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.3.1"
  }
}